<div class="modal-dialog modal-lg" role="document" id="noSearch">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
      <div class="alert alert-success" role="alert">
        <h4 class="modal-title" id="myModalLabel">informaci√≥n Personal</h4>
      </div>
    </div>
    <div class="modal-body">
      <strong>Nombre</strong>
      <h5>$artist.getName()</h5>

      <strong>Apellido</strong>
      #if($artist.getSurname() != $null)
        <h5>$artist.getSurname()</h5>
      #else
        <h5>El artista no contiene apellido</h5>
      #end

      <strong>Apodo</strong>
      #if ($artist.getNickname() != $null)
        <h5>$artist.getNickname()</h5>
      #else
        <h5>El artista no contiene apodo</h5>
      #end
    </div>
    <div class="modal-footer">
      <a href="/artists" type="button" class="btn btn-success" data-dismiss="modal">Volver</a>
    </div>
  </div>
</div>